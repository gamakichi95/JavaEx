	public class Start {
    static int[] f(int [] array, int nb) {
        array = new int[nb];
        for(int value: array)
            System.out.println(++value + " ");
        return array;
    }
    public static void main(String[] args) {
        int size = 3;
        int [] array = null;
        array = f(array, size);
        for(int value: array)
            System.out.println(value + " ");
    }